import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();

        if(n % 3 == 0 && n % 5 == 0)
            System.out.println("ThalaThalapathy");
        else if(n % 3 == 0)
            System.out.println("Thala");
        else if(n % 5 == 0)
            System.out.println("Thalapathy");
        else
            System.out.println(n);
    }
}
